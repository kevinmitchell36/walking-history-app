<h1>Events:</h1>
<% @events.each do |event| %>
<h2> <%= event.name %> </h2>
<p> <%= event.date %> </p>
<p> <%= event.time_of_day %> </p>
<p> <%= event.description_level_1 %> </p>
<p> <%= event.description_level_2 %> </p>
<p> <%= event.description_level_3 %> </p>
<p> <%= event.year %> </p>
<a href="/events/<%= event.id %>">Learn More</a>
<% end %>